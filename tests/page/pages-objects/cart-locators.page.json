{
  "locators": {
    "pageTitle": {
      "selector": ".title",
      "description": "Page title"
    },
    "itemCartList": {
      "selector": ".cart_item",
      "description": "Item cart list"
    },
    "itemNameList": {
      "selector": ".inventory_item_name",
      "description": "Item name list"
    },
    "itemPriceList": {
      "selector": ".inventory_item_price",
      "description": "Item price list"
    },
    "itemName": {
      "selector": ".inventory_item_name >> nth=${value}",
      "description": "Item name"
    },
    "itemPrice": {
      "selector": ".inventory_item_price >> nth=${value}",
      "description": "Item price"
    },
    "buttonCheckout": {
      "selector": "[data-test='checkout']",
      "description": "Checkout button"
    }
  }
}
