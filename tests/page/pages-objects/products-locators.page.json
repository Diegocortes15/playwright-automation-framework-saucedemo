{
  "locators": {
    "titlePage": {
      "selector": ".title",
      "description": "Page title"
    },
    "dropdownProductSort": {
      "selector": "[data-test='product_sort_container']",
      "description": "Dropdown product sort"
    },
    "itemList": {
      "selector": ".inventory_item",
      "description": "Item list"
    },
    "itemNameList": {
      "selector": ".inventory_item_name",
      "description": "Item name list"
    },
    "itemPriceList": {
      "selector": ".inventory_item_price",
      "description": "Item price list"
    },
    "itemBtnList": {
      "selector": "button.btn_inventory",
      "description": "Item button list"
    },
    "itemName": {
      "selector": ".inventory_item_name >> nth=${value}",
      "description": "Item name"
    },
    "itemPrice": {
      "selector": ".inventory_item_price >> nth=${value}",
      "description": "Item price"
    },
    "itemBtn": {
      "selector": "button.btn_inventory >> nth=${value}",
      "description": "Item button"
    }
  }
}
